package com.peter.packets;

import java.util.ArrayList;
import java.util.HashMap;

import com.peter.inventory.Item;

public class ChestPacket {
	public HashMap<Integer, ItemPacket> items;
	public String name
	public int x, y;
	public Integer ID;

	public ChestPacket(){}
	public ChestPacket(String name, int x, int y, int ID){
		this.name = name;
		this.x = x;
		this.y = y;
		this.ID = ID;
	}
}