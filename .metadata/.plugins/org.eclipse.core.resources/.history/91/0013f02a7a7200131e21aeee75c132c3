package com.peter.packets;

public class ItemPacket implements Comparable<ItemPacket>{
	public String name;
	public String type;
	public Integer ID;
	public int floor;
	public ItemPacket(){}
	public ItemPacket(String name, Integer ID, int floor){
		this.name = name;
		this.ID = ID;
		this.floor = floor;
	}
	
	@Override
	public int compareTo(ItemPacket item) {
		if(item.name.equals(name) && item.ID == ID)
			return 0;
		return 1;
	}
}
