package com.peter.packets;

import java.util.ArrayList;

import com.peter.inventory.Item;

public class ChestPacket {
	private ArrayList<ItemPacket> items;
	public Integer ID;

	public ChestPacket(){}
	public ChestPacket(String name, int x, int y, int ID){
		this.name = name;
		this.x = x;
		this.y = y;
		this.ID = ID;
	}
}