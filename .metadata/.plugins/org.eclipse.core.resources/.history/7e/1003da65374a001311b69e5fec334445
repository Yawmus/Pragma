package com.peter.rogue.inventory;

import com.peter.rogue.entities.Entity;

public class Item extends Entity{
	private int weight;
	private int value;
	
	public Item(String name, int weight, int value, String filename){
		super(filename, "Item");
		this.name = name;
		this.weight = weight;
		this.value = value;
	}
	public int getWeight() {
		return weight;
	}
	public int getValue(){
		return value;
	}
}