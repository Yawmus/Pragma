package com.peter.packets;

public class ItemPacket {
	public String name;
	public int x, y;
	public Integer ID;
	public ItemPacket(){}
	public ItemPacket(String name, int x, int y, int ID){
		this.name = name;
		this.x = x;
		this.y = y;
		this.ID = ID;
	}
	public ItemPacket(String name){
		this.name = name;
		x = y = ID = 0;
	}
}
