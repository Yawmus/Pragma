package com.peter.entities;

import com.peter.inventory.Food;
import com.peter.inventory.Item;
import com.peter.inventory.Wearable;
import com.peter.packets.ItemPacket;

public class PacketToObject{
	private Item temp;
	public Item itemConvert(ItemPacket item){
		switch(item.name){
		case "Gold":
			return new Item(Item.GOLD);
		case "Gem":
			temp = new Item(Item.GEM);
			temp.setPosition(item.x, item.y);
			return temp;
		case "Breast Plate":
			temp = new Item(Wearable.BREAST_PLATE);
			temp.setPosition(item.x, item.y);
			return temp;
		case "Hat":
			temp = new Item(Wearable.HAT);
			temp.setPosition(item.x, item.y);
			return temp;
		case "Ring":
			temp = new Item(Wearable.RING);
			temp.setPosition(item.x, item.y);
			return temp;
		case "Shoes":
			temp = new Item(Wearable.SHOES);
			temp.setPosition(item.x, item.y);
			return temp;
		case "Helmet":
			temp = new Item(Wearable.HELMET);
			temp.setPosition(item.x, item.y);
			return temp;
		case "Bread":
			temp = new Item(Food.BREAD);
			temp.setPosition(item.x, item.y);
			return temp;
		case "Meat":
			temp = new Item(Food.MEAT);
			temp.setPosition(item.x, item.y);
			return temp;
		}
		return null;
	}
}
