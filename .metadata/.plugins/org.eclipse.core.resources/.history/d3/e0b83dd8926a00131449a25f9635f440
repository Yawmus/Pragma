package com.peter.entities;

import com.badlogic.gdx.graphics.Color;

public class Monster extends NPC{
	
	public Monster(int level, String race, String type, String name) {
		super("tilda.png", level, race, type, name);
		if(level > 10)
			setColor(Color.RED);
		if(level > 3)
			setColor(Color.RED);
	}
}