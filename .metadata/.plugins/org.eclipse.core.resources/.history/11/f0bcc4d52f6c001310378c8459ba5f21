package com.peter.entities;

public class Player extends Entity{

	private String picture;
	public Color color;
	
	public Player(){
		super("Human", "Player");
		color = new Color();
		stats.setHitpoints(20);
	}

	public void setPicture(String picture) {
		this.picture = picture;
	}
	public String getPicture(){
		return picture;
	}
}