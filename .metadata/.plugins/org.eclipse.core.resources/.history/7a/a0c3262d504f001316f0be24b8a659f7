package com.peter.rogue.data;

import com.peter.rogue.inventory.Item;

public class ItemEntry{
	public Item item;
	public float x, y;
	
	public ItemEntry(Item item, float x, float y){
		this.item = item;
		this.x = x;
		this.y = y;
		
		pickedUp = false;
		map.put(ID, this);
		map.setMark(ID, x * tileWidth, y * tileHeight);
	}
}